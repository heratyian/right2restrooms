<div id="user-profile">
  <h1> <%=@user.email%>'s Favorite Restrooms</h1> 
  <%= button_to "logout", destroy_user_session_path, method: :delete  %>
  <% @favorite_bathrooms.each do |bathroom| %>
    <%= render bathroom %>
    <p>
      <%= link_to "Show this bathroom", bathroom %>
    </p>
    <br>
  <% end %>
</div>
